<h1>Books</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan= "3"></th>
    </tr>
    
    <tbody>
      
    </tbody>
  </thead>
  
</table>


<div class= "table">
</div>
<h2>New Book</h2>
<%= form_with model: @Book, local: true do |f| %>
  <h3>Title</h3>
  <%= f.text_field :title %>
  
  <h3>Body</h3>
  <%= f.text_area :body %>
  
  <%= f.submit"Create Book" %>
<% end %>